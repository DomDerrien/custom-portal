html, body {
	width: 100%;
	height: 100%;
	border: 0;
	padding: 0;
	margin: 0;
}

body {
	visibility: hidden;
}

.commonIcons {
	display: inline-block;
	background-image: embedurl('../images/commonIconsObjActEnabled.png');
	background-repeat: no-repeat;
	width: 16px;
	height: 16px;
	text-align: center;
}
.commonIconsAdd {
	background-position: -128px top;
}
.commonIconsUpdate {
	background-position: -144px top;
}
.commonIconsDelete {
	background-position: -80px top;
}

.categories {
	margin-bottom: 10px;
}
